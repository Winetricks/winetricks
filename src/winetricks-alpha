    fi

    w_download "http://mozilla-mirror.naist.jp//firefox/releases/4.0b9/win32/en-US/Firefox%20Setup%204.0%20Beta%209.exe" a80780d52aa8e1a8e5d9c9ffd4238441cb7d1a52 "Firefox Setup 4.0 Beta 9.exe"
    cd "$W_CACHE"/firefox4
    if test $W_OPT_UNATTENDED = 1
    then
