{
    w_download http://www.spotify.com/download/Spotify%20Installer.exe f26712b576baa1c78112a05474293deef39f7f62 SpotifyInstaller.exe

    cd "$W_CACHE"/spotify

    w_ahk_do "
        SetTitleMatchMode, 2
        run SpotifyInstaller.exe
        WinWait, Spotify 0.4.9 Setup, Location
        u = $W_OPT_UNATTENDED
        if ( u > 0 ) {
            Sleep 500
            ControlClick Button7 ; Start after install
            Sleep 500
            ControlClick Button2 ; Install
            WinWait, Spotify 0.4.9 Setup, Complete
            Sleep 500
