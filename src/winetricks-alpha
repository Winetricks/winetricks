   title="Spotify - All the music, all the time" \
   publisher="Spotify" \
   year="2011" \
   media="download" \
   file1="SpotifyInstaller.exe" \
   installed_exe1="$W_PROGRAMS_X86_WIN/Spotify/spotify.exe"

